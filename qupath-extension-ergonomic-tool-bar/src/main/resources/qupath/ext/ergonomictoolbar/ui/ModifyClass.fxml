                     <font>
                        <Font size="16.0" />
                     </font>
                        </Button>
               </children>
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin>
            </VBox>
         </center>
         <bottom>
                  <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="226.0" textAlignment="CENTER" textFill="RED" BorderPane.alignment="CENTER" />
